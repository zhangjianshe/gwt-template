<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
             xmlns:g='urn:import:com.google.gwt.user.client.ui'
             xmlns:widget="urn:import:cn.mapway.gwt_template.client.widget"
             xmlns:buttons="urn:import:cn.mapway.ui.client.widget.buttons">
    <ui:style>
        .top {
            display: flex;
            align-items: center;
            justify-content: space-between;
            padding: 0 10px;
        }
    </ui:style>
    <g:DockLayoutPanel>
        <g:north size="50">
            <g:HTMLPanel addStyleNames="{style.top}">
                <widget:Head>SSH Public Keys</widget:Head>
                <buttons:AiButton ui:field="btnUpload">上传</buttons:AiButton>
            </g:HTMLPanel>
        </g:north>
        <g:center>
            <g:ScrollPanel>
                <g:HTMLPanel ui:field="list">

                </g:HTMLPanel>
            </g:ScrollPanel>
        </g:center>
    </g:DockLayoutPanel>
</ui:UiBinder>