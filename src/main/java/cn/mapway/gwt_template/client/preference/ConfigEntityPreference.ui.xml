<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
             xmlns:g='urn:import:com.google.gwt.user.client.ui'
             xmlns:cloudflare="urn:import:cn.mapway.gwt_template.client.preference.cloudflare">
    <ui:style>
        .pad {
            padding: 10px;
        }</ui:style>
    <g:DockLayoutPanel ui:field="root">
        <g:center>
            <g:ScrollPanel addStyleNames="{style.pad}">
                <g:VerticalPanel ui:field="list" spacing="8">
                    <cloudflare:CloudflareZoneConfigPreference ui:field="cloudflareConfig"/>
                </g:VerticalPanel>
            </g:ScrollPanel>
        </g:center>
    </g:DockLayoutPanel>
</ui:UiBinder>