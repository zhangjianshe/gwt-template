<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
             xmlns:g='urn:import:com.google.gwt.user.client.ui' xmlns:widget="urn:import:cn.mapway.ui.client.widget"
>
    <g:DockLayoutPanel unit='PX'>
        <g:north size="50">
            <g:HTMLPanel addStyleNames="ai-bar">
                <widget:Header>WEB钩子</widget:Header>
                <g:Button ui:field="btnAdd" text="创建钩子"/>
            </g:HTMLPanel>
        </g:north>
        <g:center>
            <g:ScrollPanel addStyleNames="ai-border-top">
                <g:VerticalPanel ui:field="listContainer" width="100%"/>
            </g:ScrollPanel>
        </g:center>
    </g:DockLayoutPanel>
</ui:UiBinder>