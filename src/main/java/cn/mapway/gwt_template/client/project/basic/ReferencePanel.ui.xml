<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
             xmlns:g='urn:import:com.google.gwt.user.client.ui'
             xmlns:widget2="urn:import:cn.mapway.ui.client.widget"
             xmlns:list="urn:import:cn.mapway.ui.client.widget.list"
             xmlns:widget="urn:import:cn.mapway.gwt_template.client.widget">
    <g:DockLayoutPanel>
        <g:north size="60">
            <g:HorizontalPanel height="40px" width="100%" verticalAlignment="middle" spacing="10">
                <widget:Head>切换分支和标签</widget:Head>
                <widget2:SearchBox ui:field="searchBox"/>
            </g:HorizontalPanel>
        </g:north>
        <g:center>
            <g:TabLayoutPanel ui:field="tab" barHeight="50">
                <g:tab>
                    <g:header>分支</g:header>
                    <g:ScrollPanel>
                        <list:CommonList ui:field="branchList"/>
                    </g:ScrollPanel>
                </g:tab>
                <g:tab>
                    <g:header>标签</g:header>
                    <g:ScrollPanel>
                        <list:CommonList ui:field="tagList"/>
                    </g:ScrollPanel>
                </g:tab>
            </g:TabLayoutPanel>
        </g:center>
    </g:DockLayoutPanel>
</ui:UiBinder>