<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
             xmlns:g='urn:import:com.google.gwt.user.client.ui'
             xmlns:widget="urn:import:cn.mapway.gwt_template.client.widget">
    <ui:style>
        .top {
            display: flex;
            align-items: center;
            justify-content: space-between;
            border-bottom: solid 1px #e0e0e0;
            padding: 0 15px;
        }
    </ui:style>
    <g:DockLayoutPanel >
        <g:north size="50">
            <g:HTMLPanel addStyleNames="{style.top}">
                <g:HorizontalPanel ui:field="buttons" spacing="0">
                    <widget:IconButton height="50px" ui:field="btnBasic"/>
                    <widget:IconButton height="50px" ui:field="btnCode"/>
                    <widget:IconButton height="50px" ui:field="btnWiki"/>
                </g:HorizontalPanel>
                <widget:IconButton height="50px" ui:field="btnSetting"/>
            </g:HTMLPanel>
        </g:north>
        <g:center>
            <g:LayoutPanel ui:field="content">

            </g:LayoutPanel>
        </g:center>
    </g:DockLayoutPanel>
</ui:UiBinder>