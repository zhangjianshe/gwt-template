<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
             xmlns:g='urn:import:com.google.gwt.user.client.ui'
             xmlns:dialog="urn:import:cn.mapway.ui.client.widget.dialog"
             xmlns:tree="urn:import:cn.mapway.ui.client.widget.tree"
             xmlns:widget2="urn:import:cn.mapway.gwt_template.client.widget"
             xmlns:widget="urn:import:cn.mapway.ui.client.widget">
    <g:DockLayoutPanel>
        <g:south size="68">
            <dialog:SaveBar ui:field="saveBar" saveText="应用"/>
        </g:south>
        <g:west size="250">
            <g:ScrollPanel addStyleNames="ai-flex-panel">
                <tree:Tree width="100%" ui:field="list"/>
            </g:ScrollPanel>
        </g:west>
        <g:center>
            <g:ScrollPanel>
                <g:HTMLPanel addStyleNames="a-flex-panel">
                    <g:VerticalPanel spacing="10" horizontalAlignment="center">
                        <widget2:Head ui:field="tip"/>
                        <g:HorizontalPanel spacing="5" verticalAlignment="middle">
                            <widget2:Head>新的IP</widget2:Head>
                            <widget:AiTextBox ui:field="txtIp"/>
                        </g:HorizontalPanel>
                    </g:VerticalPanel>

                </g:HTMLPanel>
            </g:ScrollPanel>
        </g:center>
    </g:DockLayoutPanel>
</ui:UiBinder>