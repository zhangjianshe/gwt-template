<!DOCTYPE module PUBLIC "-//Google Inc.//DTD Google Web Toolkit 2.8.0//EN"
        "https://gwtproject.org/doctype/2.12.2/gwt-module.dtd">
<module rename-to="app">

    <inherits name="com.google.gwt.core.Core"/>
    <inherits name='com.google.gwt.user.User'/>
    <inherits name="elemental2.core.Core"/>
    <inherits name="elemental2.dom.Dom"/>
    <inherits name="jsinterop.base.Base"/>
    <inherits name="cn.mapway.ui.MapwayBase"/>
    <inherits name="cn.mapway.rbac.MapwayRbac"/>
    <source path="shared"/>
    <source path="client"/>

    <generate-with
            class="cn.mapway.ui.server.mvc.ModuleFactoryGenerator">
        <when-type-assignable
                class="cn.mapway.ui.client.mvc.ModuleFactory"/>
    </generate-with>
    
    <generate-with
            class="cn.mapway.ui.server.mvc.AttributeEditorGenerator">
        <when-type-assignable
                class="cn.mapway.ui.client.mvc.attribute.editor.IAttributeEditorFactory"/>
    </generate-with>
    <entry-point class='cn.mapway.gwt_template.client.ClientApp'/>
</module>